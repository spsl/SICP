#lang racket

(define (append seq x)
    (if (null? seq)
        (list x)
        (cons (car seq)
              (append (cdr seq) x))))
   
   
   
(define (same-parity x . y)
  (let ((tmp (list x))
        (K (remainder x 2)))
    (define (iter z result)
      (if (null? z)
          result
          (iter (cdr z)
                (if (= (remainder (car z) 2)K)
                    (append result (car z))
                    result))))
    (iter y tmp)))
