#lang racket

(define (union-set x1 x2)
  (cond ((null? x1) x2)
        ((null? x2) x1)
        ((= (car x1)
            (car x2))
         (cons (car x1)
               (union-set (cdr x1) (cdr x2))))
        ((< (car x1) (car x2))
         (cons (car x1)
               (union-set (cdr x1) x2)))
        ((> (car x1) (car x2))
         (cons (car x2)
               (union-set x1 (cdr x2))))))
