#lang slideshow
;定义product
(define (product term a next b)
  (define (iter a result)
    (if (> a b)
        result
        (iter (next a)
              (* (term a)
                 result))))
  (iter a 1))

;使用product定义factorial
(define (factorial n)
  (define (f x)x)
  (define (inc x) (+ 1 x))
  (product f 1  inc n))

